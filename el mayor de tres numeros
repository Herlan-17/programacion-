     #include "stdafx.h"
     #include <conio.h>
     #include <iostream>

        using namespace std; 
        void main() 

 {
	    int a,b,c;
	    cout << "ingrese el primer numero";
	    cin >> a;
		cout << "ingrese el segundo numero";
		cin >> b;
		cout << "ingrese el tercer numero";
		cin >> c;
        if ((a>=b)&&(a>=c))
	    cout<< "el mayor es:"<<a;
	    else
		if ((b>=a)&&(b>=c))
		cout <<"el mayor es:" <<b;
		else 
	    cout<< "el mayores:"<<c;

	    getch ();
}
